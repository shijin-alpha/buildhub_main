# Collaborative AI Pipeline Environment Configuration
# Copy this file to .env and fill in your API keys

# Google Gemini API Key (Required for Stage 3: Design Description Generation)
# Get your API key from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Optional: Hugging Face API Token (for alternative diffusion models)
# HUGGINGFACE_API_TOKEN=your_hf_token_here

# Optional: OpenAI API Key (for alternative language models)
# OPENAI_API_KEY=your_openai_api_key_here

# Service Configuration
AI_SERVICE_HOST=127.0.0.1
AI_SERVICE_PORT=8000

# Model Configuration
DIFFUSION_MODEL_ID=runwayml/stable-diffusion-v1-5
YOLO_MODEL_SIZE=nano

# Performance Settings
TORCH_DEVICE=auto
ENABLE_ATTENTION_SLICING=true
ENABLE_CPU_OFFLOAD=false

# Security Settings
ALLOWED_ORIGINS=*
MAX_FILE_SIZE=5242880
ALLOWED_FILE_TYPES=image/jpeg,image/png

# Logging
LOG_LEVEL=INFO
LOG_FILE=ai_service.log